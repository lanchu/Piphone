<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FoldingMenu">
        <attr name="foldNumber" format="integer" />
    </declare-styleable>

    <declare-styleable name="DashTab">
        <attr name="font_aws" format="string" />
        <attr name="DashTab_background" format="color" />
        <attr name="tab_text_how" format="string" />
        <attr name="tab_text_what" format="string" />
        <attr name="tab_text_button" format="string" />
    </declare-styleable>


    <declare-styleable name="Tablelistview">
        <attr name="pic" format="reference" />

        <attr name="back_ground" format="color" />
        <attr name="bar_Color" format="color" />
        <attr name="title_Color" format="color" />
        <attr name="title_text" format="string" />

        <attr name="text_Color" format="color" />
        <attr name="text_Size" format="dimension" />
        <attr name="need_button" format="boolean" />
        <attr name="button_left_text" format="string" />
        <attr name="button_right_text" format="string" />
        <attr name="button_left_Color" format="string" />
        <attr name="button_right_Color" format="string" />
        <attr name="button_shape" format="reference" />

    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="progress" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="barWidth" format="dimension" />
        <attr name="barLength" format="dimension" />
    </declare-styleable>
</resources>